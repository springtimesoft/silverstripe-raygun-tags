{
    "name": "springtimesoft/silverstripe-raygun-tags",
    "description": "Automatically add Silverstripe tags to Raygun error logs",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/springtimesoft/silverstripe-raygun-tags",
    "keywords": [
        "silverstripe",
        "raygun",
        "logging"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Springtimesoft",
            "homepage": "https://springtimesoft.co.nz/"
        }
    ],
    "support": {
        "issues": "https://github.com/springtimesoft/silverstripe-raygun-tags/issues"
    },
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/raygun": "^4.0"
    },
    "conflict": {
        "axllent/silverstripe-raygun-tags": "*"
    },
    "autoload": {
        "psr-4": {
            "Springtimesoft\\RaygunTags\\": "src/"
        }
    }
}
